{
  "name": "@bkbank/ui",
  "version": "1.2.0-beta.17",
  "description": "BK Bank's Design System as a TailwindCSS plugin.",
  "main": "index.js",
  "license": "ISC",
  "files": [
    "index.js",
    "theme.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jvzaniolo/bkbank-ui.git"
  },
  "keywords": [
    "tailwindcss",
    "plugin",
    "design-system"
  ],
  "author": "joao.zaniolo",
  "bugs": {
    "url": "https://github.com/jvzaniolo/bkbank-ui/issues"
  },
  "homepage": "https://github.com/jvzaniolo/bkbank-ui#readme",
  "scripts": {
    "build": "npm-run-all --parallel build:**",
    "build:css": "postcss src/styles/*.css --dir dist",
    "build:minify:js": "esbuild --bundle ./src/scripts/* --outdir=dist --minify --sourcemap --out-extension:.js=.min.js",
    "build:minify:components": "esbuild --bundle ./src/scripts/components/* --outdir=dist/components --minify --sourcemap --out-extension:.js=.min.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "cross-env": "^7.0.3",
    "postcss-js": "^4.0.1",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@heroicons/react": "^2.0.16",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/mdx2-csf": "^0.0.4",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.28",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "cssnano": "^5.1.15",
    "esbuild": "^0.17.10",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "require-from-string": "^2.0.2",
    "semantic-release": "^20.1.1",
    "typescript": "^4.9.5",
    "webpack": "5.75.0"
  }
}
