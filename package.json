{
  "name": "@bkbank/ui",
  "version": "1.2.0-beta.18",
  "description": "BK Bank's Design System as a TailwindCSS plugin.",
  "main": "index.js",
  "license": "ISC",
  "files": [
    "index.js",
    "theme.js",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jvzaniolo/bkbank-ui.git"
  },
  "keywords": [
    "tailwindcss",
    "plugin",
    "design-system"
  ],
  "author": "joao.zaniolo",
  "bugs": {
    "url": "https://github.com/jvzaniolo/bkbank-ui/issues"
  },
  "homepage": "https://github.com/jvzaniolo/bkbank-ui#readme",
  "scripts": {
    "build": "npm-run-all --parallel build:**",
    "build:css": "postcss src/styles/*.css --dir dist",
    "build:minify:js": "esbuild --bundle ./src/scripts/* --outdir=dist --minify --sourcemap --out-extension:.js=.min.js",
    "build:minify:components": "esbuild --bundle ./src/scripts/components/* --outdir=dist/components --minify --sourcemap --out-extension:.js=.min.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "postcss-js": "^4.0.1",
    "tailwindcss": "^3.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@heroicons/react": "^2.0.16",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "7.0.0-beta.61",
    "@storybook/addon-interactions": "7.0.0-beta.61",
    "@storybook/addon-links": "7.0.0-beta.61",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/blocks": "7.0.0-beta.61",
    "@storybook/react": "7.0.0-beta.61",
    "@storybook/react-webpack5": "7.0.0-beta.61",
    "@storybook/testing-library": "0.0.14-next.1",
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.13",
    "esbuild": "^0.17.10",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "^20.1.1",
    "storybook": "7.0.0-beta.61",
    "typescript": "^4.9.5"
  }
}
