{
  "name": "@bkbank/root",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:ui": "turbo run dev --filter @bkbank/ui",
    "dev:docs": "turbo run dev --filter @bkbank/docs",
    "dev:react": "turbo run dev --filter @bkbank/react",
    "storybook": "turbo run storybook",
    "lint": "turbo run lint",
    "release": "turbo run release",
    "format": "prettier --write \"**/*.{css,js,jsx,ts,tsx,md,mdx}\""
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "turbo": "^1.8.3"
  },
  "packageManager": "pnpm@7.29.0"
}
