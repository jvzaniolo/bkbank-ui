{
  "name": "@bkbank/ui",
  "version": "0.0.1-alpha",
  "description": "The Bk Bank Design System written as a TailwindCSS Plugin",
  "main": "index.js",
  "scripts": {
    "test": "exit 0",
    "prebuild": "npm run build:base && npm run build:components",
    "build": "prejss-cli dist/*.css --format commonjs",
    "build:base": "postcss --config packages/core packages/core/base/index.css -o dist/base.css",
    "build:components": "postcss --config packages/core packages/core/components/index.css -o dist/components.css",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvzaniolo/bkbank-ui.git"
  },
  "keywords": [
    "tailwindcss",
    "plugin",
    "design-system"
  ],
  "author": "joao.zaniolo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jvzaniolo/bkbank-ui/issues"
  },
  "homepage": "https://github.com/jvzaniolo/bkbank-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/html": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prejss-cli": "^0.3.3",
    "prettier": "^2.8.4"
  },
  "dependencies": {
    "postcss-js": "^4.0.1",
    "tailwindcss": "^3.2.7"
  }
}
