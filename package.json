{
  "name": "@bkbank/ui",
  "version": "0.0.0-beta.10",
  "description": "The Bk Bank Design System written as a TailwindCSS Plugin",
  "main": "index.js",
  "scripts": {
    "build": "run-p build:**",
    "build:base": "postcss --config packages/core packages/core/base/index.css -o dist/base.css",
    "build:components": "postcss --config packages/core packages/core/components/index.css -o dist/components.css",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "yarn build && yarn publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvzaniolo/bkbank-ui.git"
  },
  "keywords": [
    "tailwindcss",
    "plugin",
    "design-system"
  ],
  "author": "joao.zaniolo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jvzaniolo/bkbank-ui/issues"
  },
  "homepage": "https://github.com/jvzaniolo/bkbank-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^7.0.0-beta.52",
    "@storybook/addon-essentials": "^7.0.0-beta.52",
    "@storybook/addon-interactions": "^7.0.0-beta.52",
    "@storybook/addon-links": "^7.0.0-beta.52",
    "@storybook/addon-styling": "^0.1.0",
    "@storybook/html-vite": "^7.0.0-beta.52",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.52",
    "vite": "^4.1.3"
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.5.14",
    "postcss-js": "^4.0.1",
    "tailwindcss": "^3.2.7"
  }
}
